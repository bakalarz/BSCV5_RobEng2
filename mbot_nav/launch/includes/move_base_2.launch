<launch>
    <node pkg="move_base" type="move_base" name="move_base" output="screen">
        <param name="controller_frequency" value="10.0"/>
        <rosparam file="$(find mbot_nav)/param/explore_2/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find mbot_nav)/param/explore_2/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find mbot_nav)/param/explore_2/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find mbot_nav)/param/explore_2/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find mbot_nav)/param/explore_2/base_local_planner_params.yaml" command="load" />
    </node>

</launch>
